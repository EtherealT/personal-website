<div class="npnbnm" id="page-wrapper">
    {{page-header pageName=model.pageName}}

    <div id="projects-hold">
        {{#each model.projects as |project|}}
            <a href="{{project.url}}" class="project" target="_blank">
                <img src="{{project.imgUrl}}" />
                <div class="tags-hold">
                    {{#each project.tags as |tag|}}
                    <span class="tag">{{tag}}</span>
                    {{/each}}
                </div>
                <span class="project-title">{{project.title}}</span>
                <span class="project-description">{{project.description}}</span>
            </a>
        {{/each}}
    </div>

    <!--cant use page footer component here because its absolute and the project list height is not fixed.-->
    <div id="footer"i style="position:relative">
        <span>&copy; Tobi Adeyinka</span>
    </div>
</div>
